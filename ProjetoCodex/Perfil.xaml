<Window x:Class="ProjetoCodex.Perfil"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ProjetoCodex"
        mc:Ignorable="d"
        Title="Perfil" Height="450" Width="800">
    <Grid>
        <TextBox x:Name="textNovoNome" HorizontalAlignment="Left" Margin="84,61,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="textBoxNovoEmail" HorizontalAlignment="Left" Margin="84,108,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="textBoxNovaBio" HorizontalAlignment="Left" Margin="84,152,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120"/>
        <Button x:Name="btn_Editar" Content="Button" HorizontalAlignment="Left" Margin="425,244,0,0" VerticalAlignment="Top" Click="btn_Editar_Click"/>
        <Button Content="Button" HorizontalAlignment="Left" Margin="124,254,0,0" VerticalAlignment="Top" Click="Button_Click"/>

    </Grid>
</Window>
