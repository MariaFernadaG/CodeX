<Window x:Class="ProjetoCodex.LogoutPage"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ProjetoCodex"
        mc:Ignorable="d"
        Title="LogoutPage" Height="322" Width="498">
    <Grid Margin="0,0,0,-6">
        <Button Content="Sair " HorizontalAlignment="Left" Margin="48,194,0,0" VerticalAlignment="Top" Height="77" Width="168" Click="Button_Click"/>
        <Button Content="Voltar" HorizontalAlignment="Left" Margin="290,192,0,0" VerticalAlignment="Top" Height="79" Width="161" RenderTransformOrigin="0.5,0.5" Click="Button_Click_1">
            <Button.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform/>
                    <RotateTransform Angle="-0.05"/>
                    <TranslateTransform/>
                </TransformGroup>
            </Button.RenderTransform>
        </Button>

    </Grid>
</Window>
